<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".X5.X5WebviewActivity"
    tools:showIn="@layout/app_bar_x5_webview">

    <LinearLayout
        android:id="@+id/guanggao"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"></LinearLayout>

    <com.meizi.haokan.X5.X5WebView
        android:id="@+id/x5webview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></com.meizi.haokan.X5.X5WebView>


</LinearLayout>